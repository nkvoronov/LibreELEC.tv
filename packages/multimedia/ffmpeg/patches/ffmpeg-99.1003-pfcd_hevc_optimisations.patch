+
+        bgt      1b
+        bx       r6
+.endm
+
+.macro  edge_4bx4_e0, body_fn, pb
+        mov      r6, lr
+
+1:      subs     r12, #4
+
+        vld1.32  {d2[0]}, [r1]
+        sub      r1, #\pb
+        vld1.32  {d0[0]}, [r1]
+        add      r1, #(\pb * 2)
+        vld1.32  {d4[0]}, [r1], r3      @ R
+        vld1.32  {d4[1]}, [r1]
+        sub      r1, #\pb
+        vld1.32  {d2[1]}, [r1]
+        sub      r1, #\pb
+        vld1.32  {d0[1]}, [r1], r3      @ L
+        vld1.32  {d1[0]}, [r1]
+        add      r1, #\pb
+        vld1.32  {d3[0]}, [r1]
+        add      r1, #\pb
+        vld1.32  {d5[0]}, [r1], r3      @ R
