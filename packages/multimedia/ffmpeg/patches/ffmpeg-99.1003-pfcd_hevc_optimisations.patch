+1:      subs     r12, #2
+
+        vld1.8   {d2}, [r1, :64]
+        sub      r1, #\pb
+        vld1.8   {d0}, [r1]
+        add      r1, #(\pb * 2)
+        vld1.8   {d4}, [r1], r3
+        sub      r1, #\pb
+        vld1.8   {d3}, [r1, :64]
+        sub      r1, #\pb
+        vld1.8   {d1}, [r1]
+        add      r1, #(\pb * 2)
+        vld1.8   {d5}, [r1], r3
+        sub      r1, #\pb
+
+        bl       \body_fn
+
+        vst1.8   {d0}, [r0, :64], r2
