+        add      r1, #\pb
+        vld1.32  {d3[1]}, [r1], r3      @ M
+
+        bl       \body_fn
+
+        vst1.32  {d0[0]}, [r0], r2
+        vst1.32  {d0[1]}, [r0], r2
+        vst1.32  {d1[0]}, [r0], r2
