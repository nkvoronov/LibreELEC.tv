. /etc/profile
oe_setup_addon service.acestream-engine

chmod +x $ADDON_DIR/bin/*
chmod a+x $ADDON_DIR/share/acestream/acestreamengine
chmod a+x $ADDON_DIR/share/acestream/start-engine

$ADDON_DIR/share/acestream/start-engine "$@"