      <text refresh="all,list,timeout" x1="30" x2="1740" y1="5" y2="41" color="FontColorActive" font="VDRSymbols Sans:Book@35" >{MenuCurrent}</text>
     </list>
     <!-- Messages -->
     <block refresh="all,update,timeout,full" changed="{MessageInfo}{MessageStatus}{MessageWarning}{MessageError}" condition="and({Message},or(equal({MenuTitle:clean},trans('Recordings')),equal({MenuTitle:clean},trans('Timers'))) )" >
     <rectangle x1="80" x2="1840" y1="135" y2="180" condition="{MessageInfo}" color="MainSolid" />
     <rectangle x1="80" x2="1840" y1="135" y2="180" condition="{MessageStatus}" color="Status" />
     <rectangle x1="80" x2="1840" y1="135" y2="180" condition="{MessageWarning}" color="Warning" />
     <rectangle x1="80" x2="1840" y1="135" y2="180" condition="{MessageError}" color="Error" />
     <text x1="136" x2="1840" y1="136" y2="179" color="White" font="VDRSymbols Sans:Book@35" >{MessageInfo}</text>
     <text x1="136" x2="1840" y1="136" y2="179" color="White" font="VDRSymbols Sans:Book@35" >{MessageStatus}</text>
     <text x1="136" x2="1840" y1="136" y2="179" color="White" font="VDRSymbols Sans:Book@35" >{MessageWarning}</text>
     <text x1="136" x2="1840" y1="136" y2="179" color="White" font="VDRSymbols Sans:Book@35" >{MessageError}</text>
     </block>

     <!-- TIMERSWATCH --> <block refresh="all,update,timeout,list,full" changed="{MenuTitle}{DateTime}{MessageInfo}{MessageStatus}{MessageWarning}{MessageError}" condition="or(equal({MenuTitle:clean},trans('Recordings')),equal({MenuTitle:clean},trans('Timers')))" >    
     <!-- TIMERSWATCH -->  <text  x1="1000" x2="1200" y1="80" y2="115" color="FontColor" align="right" font="VDRSymbols Sans:Book@35" >{DateTime:%H\:%M} </text>
     <!-- TIMERSWATCH -->  <text  x1="1205" x2="1350" y1="80" y2="115" color="FontColor" align="left" font="VDRSymbols Sans:Book@35" > </text>
     <!-- trans('Uhr') -->
     <!-- TIMERSWATCH --> </block>
     
 <!-- SUBMENU END: Recordings, Timers -->
 
 <!-- SUBMENU: EPG -->
    <block condition="{PresentTitle}" refresh="all,scroll" >
     <rectangle x1="1240" x2="1840" y1="131" y2="185" color="MainLight" />
     <rectangle x1="80" x2="1840" y1="185" y2="1004" color="MainLightMR" />
     <text  x1="1250" x2="1400" y1="80" y2="115" color="FontColor" align="right" font="VDRSymbols Sans:Book@35" >{DateTime:%H\:%M}</text>
     <text  x1="1405" x2="1500" y1="85" y2="115" color="FontColor" align="left" font="VDRSymbols Sans:Book@30" > </text>
     <!-- trans('Uhr') -->
     <text  x1="800" x2="1040" y1="80" y2="115" color="FontColor" align="right" font="VDRSymbols Sans:Book@35" >{DateTime:%A} - </text>
     <text  x1="1045" x2="1240" y1="85" y2="115" color="FontColor" align="left" font="VDRSymbols Sans:Book@30" >{DateTime:%e.%m.%Y}</text>
     
     <text x1="116" x2="1620" y1="140" y2="1004" color="FontColor" font="VDRSymbols Sans:Bold@35" >{PresentTitle}</text>
     <text x1="116" x2="1620" y1="240" y2="1004" color="FontColor" font="VDRSymbols Sans:Book@35" >{PresentStartDateTime:%a %d.%m.%y  %H\:%M} - {PresentEndDateTime:%H\:%M}  </text>
     <text x1="116" x2="1620" y1="280" y2="1004" color="FontColor" font="VDRSymbols Sans:Book@35"  condition="{PresentVPSDateTime}" >{PresentDuration:%m} мин | VPS: {PresentVPSDateTime:%H\:%M}</text>
     <text x1="116" x2="1620" y1="280" y2="1004" color="FontColor" font="VDRSymbols Sans:Book@35"  condition="not({PresentVPSDateTime})" >{PresentDuration:%m} мин </text>
     <image x1="1400" x2="1700" y1="140" y2="570" colors="64" condition="file('/var/cache/vdr/epgimages/{PresentEventID}.jpg')"  path="/var/cache/vdr/epgimages/{PresentEventID}.jpg"/>
      <!-- trans('length') -->
     
     <block  condition="not({PresentDescription})" >
      <scrolltext x1="116" x2="1840" y1="320" y2="400" color="FontColor" font="VDRSymbols Sans:Bold@30" >{PresentShortText}</scrolltext>
     </block>
     
     
     <block  condition="{PresentDescription}" >
      <text x1="116" x2="1620"  y1="320" y2="400" color="FontColor" font="VDRSymbols Sans:Bold@30" >{PresentShortText}</text>
      <scrolltext x1="116" x2="1820"  y1="400" y2="1004" color="FontColor" font="VDRSymbols Sans:Book@35" >{PresentDescription}</scrolltext>
     </block>
     <scrollbar x1="1830" x2="1839" y1="185" y2="1004" condition="or({CanScrollUp},{CanScrollDown})" color="BarFill" bgColor="Bar" />
     </block>
 <!-- SUBMENU END: EPG -->

 <!-- SUBMENU: Recording Info -->
    <block condition="{RecordingName}" refresh="all,scroll" >
     <rectangle x1="80" x2="1840" y1="185" y2="1004" color="MainLightMR" />
     <rectangle x1="1240" x2="1840" y1="131" y2="185" color="MainLight" />
     <text  x1="1250" x2="1400" y1="80" y2="115" color="FontColor" align="right" font="VDRSymbols Sans:Book@35" >{DateTime:%H\:%M}</text>
     <text  x1="1405" x2="1500" y1="85" y2="115" color="FontColor" align="left" font="VDRSymbols Sans:Book@30" > </text>
     <!-- trans('Uhr') -->
     <text  x1="800" x2="1040" y1="80" y2="115" color="FontColor" align="right" font="VDRSymbols Sans:Book@35" >{DateTime:%A} - </text>
     <text  x1="1045" x2="1240" y1="85" y2="115" color="FontColor" align="left" font="VDRSymbols Sans:Book@30" >{DateTime:%e.%m.%Y}</text>
     <text x1="116" x2="1620" y1="140" y2="1004" color="FontColor" font="VDRSymbols Sans:Bold@35" >{RecordingTitle}</text>
     <text x1="116" x2="1620" y1="240" y2="1004" color="FontColor" font="VDRSymbols Sans:Book@35" >{RecordingDateTime:%a %d.%m.%y  %H\:%M} </text>
     <text x1="116" x2="1620" y1="280" y2="1004" color="FontColor" font="VDRSymbols Sans:Book@25" >{RecordingLength} мин | cutted: {RecordingCuttedLength} мин</text>
     <!-- trans('cutted') -->
     <block  condition="not({RecordingDescription})" >
      <scrolltext x1="116" x2="1620" y1="320" y2="1004" color="FontColor" font="VDRSymbols Sans:Bold@35" >{RecordingShortText}</scrolltext>
     </block>
     <block  condition="not({RecordingShortText})" >
      <text x1="116" x2="1620" y1="345" y2="1004" color="FontColor" font="VDRSymbols Sans:Bold@35" >{RecordingTitle}</text>
     </block>
     <block  condition="{RecordingDescription}" >
      <text x1="116" x2="1620" y1="345" y2="1004" color="FontColor" font="VDRSymbols Sans:Bold@35" >{RecordingShortText}</text>
      <scrolltext x1="116" x2="1620" y1="385" y2="1004" color="FontColor" font="VDRSymbols Sans:Book@35" >{RecordingDescription}</scrolltext>
     </block>
     <scrollbar x1="1830" x2="1839" y1="320" y2="1004" condition="or({CanScrollUp},{CanScrollDown})" color="BarFill" bgColor="Bar" />
    </block>
 <!-- SUBMENU END: Recording Info --> 
 
 
 <!-- SUBMENU CONTENT: MenuText (Plugins w/o list) -->  
     <block condition="{MenuText}" refresh="all,update,timeout,full,scroll" changed="{MenuText}">
     <scrolltext x1="116" x2="1640" y1="185" y2="1004" font="VDRSymbols Sans:Book@32" color="FontColor" >{MenuText}</scrolltext>
     <scrollbar  x1="1830" x2="1839" y1="185" y2="1000" condition="or({CanScrollUp},{CanScrollDown})" color="BarFill" bgColor="Bar" />
    </block>
    
 <!-- SUBMENU CONTENT END: MenuText (Plugins w/o list) -->  
 
