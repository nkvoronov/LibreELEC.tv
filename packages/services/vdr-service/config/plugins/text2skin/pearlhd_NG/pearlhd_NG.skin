 <?xml version="1.0"?>
 <skin version="1.1" name="pearlhd_NG" screenBase="absolute" >


<!--###############################################################################################
 #   CHANNEL INFO         ok!                                                                     #
 ################################################################################################-->
  
  <display id="channelInfo" >

 <!-- TOP WINDOW-->
   <window x1="1300" x2="1920" y1="0" y2="70" bpp="4" />
    <rectangle x1="1300" x2="1920" y1="0" y2="70" color="MainLight" />
    <text x1="1310" x2="1840" y1="25" y2="70" color="FontColor" align="right" font="VDRSymbols Sans:Book@35" >{DateTime:%A - %e.%m.%y - %H\:%M}</text>
 <!-- TOP WINDOW END--> 
 
 
 <!-- LOWER WINDOW 1-->
   <window x1="80" x2="1840" y1="699" y2="800" bpp="4" />
<!-- HEAD -->   
    <block>
     <rectangle x1="80" x2="1840" y1="700" y2="770" color="MainSolid" />
     <rectangle x1="80" x2="100" y1="770" y2="795" color="MainSolid" />
     <rectangle x1="1820" x2="1840" y1="770" y2="795" color="MainSolid" />
     <rectangle x1="100" x2="1820" y1="770" y2="795" color="Bar" />
     <progress  x1="100" x2="1820" y1="770" y2="795" color="BarFill" bgColor="Bar" current="{PresentProgress}" total="{PresentDuration}" />
     <rectangle x1="80" x2="1840" y1="795" y2="800" color="MainSolid" />
    </block>
 <!-- HEAD END--> 
 
 <!-- BODY -->
 <!-- SYMBOLS --> 
    <block>
     
      <text x1="390" x2="1620" y1="705" y2="755" color="FontColor" condition="not({ChannelNumber})" align="center" font="VDRSymbols Sans:Book@50" >{ChannelName} </text>
      <text x1="390" x2="1620" y1="705" y2="755" color="FontColor" condition="{ChannelNumber}" align="center" font="VDRSymbols Sans:Book@50" >{ChannelNumber} - {ChannelName} </text>
 
     <block>
     <text x1="140" x2="290" y1="772" y2="792" align="right" color="MainSolid" font="VDRSymbols Sans:Bold@20" >vdr</text>
     <text x1="291" x2="320" y1="770" y2="782" align="left" color="MainSolid" font="VDRSymbols Sans:Bold@12" >HD</text>
     </block>

     <block>
     <text x1="1400" x2="1815" y1="772" y2="792" align="right" color="MainSolid" font="VDRSymbols Sans:Bold@20" >VDR-2.1.6</text>
     </block>


     <block x1="100" x2="150" y1="720" y2="750" condition="not({HasTeletext})" >
      <rectangle x1="1" x2="35" y1="1" y2="26" color="Divider" />
      <rectangle x1="2" x2="34" y1="2" y2="25" color="MainLight" />
      <rectangle x1="4" x2="32" y1="5" y2="7" color="Divider" />
      <rectangle x1="4" x2="32" y1="10" y2="12" color="Divider" />
      <rectangle x1="4" x2="32" y1="15" y2="17" color="Divider" />
      <rectangle x1="4" x2="32" y1="20" y2="22" color="Divider" />
     </block>
     <block x1="100" x2="150" y1="720" y2="750" condition="{HasTeletext}" >
      <rectangle x1="1" x2="35" y1="1" y2="26" color="Green" />
      <rectangle x1="2" x2="34" y1="2" y2="25" color="MainLight" />
      <rectangle x1="4" x2="32" y1="5" y2="7" color="Green" />
      <rectangle x1="4" x2="32" y1="10" y2="12" color="Green" />
      <rectangle x1="4" x2="32" y1="15" y2="17" color="Green" />
      <rectangle x1="4" x2="32" y1="20" y2="22" color="Green" />
      </block>
     <block x1="155" x2="205" y1="720" y2="750" condition="not({HasDolby})" >
      <rectangle x1="1" x2="35" y1="1" y2="26" color="Divider" />
      <rectangle x1="2" x2="34" y1="2" y2="25" color="MainLight" />
      <rectangle x1="18" x2="18" y1="2" y2="26" color="Divider" />
      <ellipse x1="4" x2="16" y1="4" y2="22" arc="5"  color="Divider" />
      <ellipse x1="20" x2="32" y1="4" y2="22" arc="7"  color="Divider" />
     </block>
     <block x1="155" x2="205" y1="720" y2="750" condition="{HasDolby}" >
      <block >
       <rectangle x1="1" x2="35" y1="1" y2="26" color="Yellow" />
       <rectangle x1="2" x2="34" y1="2" y2="25" color="MainLight" />
       <rectangle x1="18" x2="18" y1="2" y2="26" color="Yellow" />
       <ellipse x1="4" x2="16" y1="4" y2="22" arc="5"  color="Yellow" />
       <ellipse x1="20" x2="32" y1="4" y2="22" arc="7"  color="Yellow" />
      </block> 
      <block condition="equal({Language},'deuDD')" >
       <rectangle x1="1" x2="35" y1="1" y2="26" color="Green" />
       <rectangle x1="2" x2="34" y1="2" y2="25" color="MainLight" />
       <rectangle x1="18" x2="18" y1="2" y2="26" color="Green" />
       <ellipse x1="4" x2="16" y1="4" y2="22" arc="5"  color="Green" />
       <ellipse x1="20" x2="32" y1="4" y2="22" arc="7"  color="Green" />
      </block>
      <block condition="equal({Language},'ddDD')" >
       <rectangle x1="1" x2="35" y1="1" y2="26" color="Green" />
       <rectangle x1="2" x2="34" y1="2" y2="25" color="MainLight" />
       <rectangle x1="18" x2="18" y1="2" y2="26" color="Green" />
       <ellipse x1="4" x2="16" y1="4" y2="22" arc="5"  color="Green" />
       <ellipse x1="20" x2="32" y1="4" y2="22" arc="7"  color="Green" />
      </block>
      
     </block>
     <block x1="210" x2="260" y1="720" y2="750" condition="not({HasMultilang})" >
      <rectangle x1="1" x2="35" y1="1" y2="26" color="Divider" />
      <rectangle x1="2" x2="34" y1="2" y2="25" color="MainLight" />
      <ellipse x1="5" x2="18" y1="6" y2="22" color="Divider" />
      <ellipse x1="5" x2="18" y1="8" y2="20" color="Divider" />
      <ellipse x1="19" x2="32" y1="6" y2="22" color="Divider" />
      <ellipse x1="19" x2="32" y1="8" y2="20" color="MainLight" />
     </block>
     <block x1="210" x2="260" y1="720" y2="750" condition="{HasMultilang}" >
      <block condition="equal({AudioChannel},'stereo')" >
       <rectangle x1="1" x2="35" y1="1" y2="26" color="Green" />
       <rectangle x1="2" x2="34" y1="2" y2="25" color="MainLight" />
       <ellipse x1="5" x2="18" y1="6" y2="22" color="Green" />
       <ellipse x1="5" x2="18" y1="8" y2="20" color="Green" />
       <ellipse x1="19" x2="32" y1="6" y2="22" color="Green" />
       <ellipse x1="19" x2="32" y1="8" y2="20" color="Green" />
      </block>
      <block condition="equal({AudioChannel},'left')" >
       <rectangle x1="1" x2="35" y1="1" y2="26" color="Green" />
       <rectangle x1="2" x2="34" y1="2" y2="25" color="MainLight" />
       <ellipse x1="5" x2="18" y1="6" y2="22" color="Green" />
       <ellipse x1="5" x2="18" y1="8" y2="20" color="Green" />
       <ellipse x1="19" x2="32" y1="6" y2="22" color="Green" />
       <ellipse x1="19" x2="32" y1="8" y2="20" color="MainLight" />
      </block>
      <block condition="equal({AudioChannel},'right')" >
       <rectangle x1="1" x2="35" y1="1" y2="26" color="Green" />
       <rectangle x1="2" x2="34" y1="2" y2="25" color="MainLight" />
       <ellipse x1="5" x2="18" y1="6" y2="22" color="Green" />
       <ellipse x1="5" x2="18" y1="8" y2="20" color="MainLight" />
       <ellipse x1="19" x2="32" y1="6" y2="22" color="Green" />
       <ellipse x1="19" x2="32" y1="8" y2="20" color="Green" />
      </block>
     </block>
     <block x1="265" x2="315" y1="720" y2="750" condition="not({IsEncrypted})" >
      <rectangle x1="1" x2="35" y1="1" y2="26" color="Divider" />
      <rectangle x1="2" x2="34" y1="2" y2="25" color="MainLight" />
      <ellipse x1="19" x2="32" y1="9" y2="23" color="Divider" />
      <ellipse x1="22" x2="29" y1="12" y2="20" color="MainLight" />
      <rectangle x1="4" x2="22" y1="9" y2="11" color="Divider" />
      <rectangle x1="6" x2="8" y1="11" y2="15" color="Divider" />
      <rectangle x1="10" x2="12" y1="11" y2="15" color="Divider" />
     </block>
     <block x1="265" x2="315" y1="720" y2="750" condition="{IsEncrypted}" >
      <rectangle x1="1" x2="35" y1="1" y2="26" color="Green" />
      <rectangle x1="2" x2="34" y1="2" y2="25" color="MainLight" />
      <ellipse x1="19" x2="32" y1="9" y2="23" color="Green" />
      <ellipse x1="22" x2="29" y1="12" y2="20" color="MainLight" />
      <rectangle x1="4" x2="22" y1="9" y2="11" color="Green" />
      <rectangle x1="6" x2="8" y1="11" y2="15" color="Green" />
      <rectangle x1="10" x2="12" y1="11" y2="15" color="Green" />
     </block>
     <block x1="320" x2="370" y1="720" y2="750" condition="not({IsRecording})" >
      <rectangle x1="1" x2="35" y1="1" y2="26" color="Divider" />
      <rectangle x1="2" x2="34" y1="2" y2="25" color="MainLight" />
      <blink x1="4" x2="36" y1="3" y2="27" color="Warning" delay="400" condition="{TimerConflicts}" font="VDRSymbols Sans:Bold@16" >rec</blink>
      <text x1="4" x2="36" y1="3" y2="27" color="Divider" condition="not({TimerConflicts})"  font="VDRSymbols Sans:Bold@16" >rec</text>
     </block>
     <block x1="320" x2="370" y1="720" y2="750" condition="{IsRecording}" >
      <rectangle x1="1" x2="35" y1="1" y2="26" color="Red" />
      <rectangle x1="2" x2="34" y1="2" y2="25" color="MainLight" />
      <blink x1="4" x2="36" y1="3" y2="27" color="Warning" delay="800" condition="{TimerConflicts}" font="VDRSymbols Sans:Bold@16" >rec</blink>
      <text x1="4" x2="36" y1="3" y2="27" color="Red" condition="not({TimerConflicts})" font="VDRSymbols Sans:Bold@16" >rec</text>
     </block>
 <!-- SYMBOLS END --> 
 <!-- BODY END --> 
   </block>
 
 <!-- LOWER WINDOW 1 END-->


 <!-- LOWER WINDOW 2-->
   <window x1="80" x2="1840" y1="801" y2="1040" bpp="8" />

     <rectangle x1="80" x2="1840" y1="801" y2="1040" color="MainSolid" />

 <!-- BODY -->
 <!-- SYMBOLS -->
 <!-- CHANNELLOGO -->    
 <image x1="100" x2="370" y1="820" y2="1020" colors="255" condition="file('/media/Media/picons/vdr/{ChannelName}.png')" path="/media/Media/picons/vdr/{ChannelName}.png" />
 <!-- SYMBOLS END -->
 <!-- BODY END -->

     <block>
 <!-- PRESENT-->
     <text x1="390" x2="490" y1="820" y2="855" color="FontColor" font="VDRSymbols Sans:Book@35" >{PresentStartDateTime:%H\:%M} </text>
     <text x1="510" x2="1620" y1="820" y2="855" color="FontColor" font="VDRSymbols Sans:Book@35" condition="not({HasTimer})" >{PresentTitle}</text>
     <text x1="510" x2="1620" y1="820" y2="855" color="FontColor" font="VDRSymbols Sans:Book@35" condition="{HasTimer}" >{PresentTitle} *</text>
 <!-- NORCHANNELLOGO -->    <text x1="1660" x2="1760" y1="825" y2="855" color="FontColor" align="right" font="VDRSymbols Sans:Book@30" condition="{PresentRemaining}" >+ {PresentProgress:%m}</text>
 <!-- NORCHANNELLOGO -->    <text x1="1765" x2="1830" y1="830" y2="855" color="FontColor" align="left" font="VDRSymbols Sans:Book@25" condition="{PresentRemaining}" >мин</text>
 <!-- NORCHANNELLOGO -->    <text x1="1660" x2="1760" y1="865" y2="895" color="FontColor" align="right" font="VDRSymbols Sans:Book@30" condition="{PresentDuration}" >{PresentDuration:%m}</text>
 <!-- NORCHANNELLOGO -->    <text x1="1765" x2="1830" y1="870" y2="895" color="FontColor" align="left" font="VDRSymbols Sans:Book@25" condition="{PresentDuration}" >мин</text>
      <text x1="510" x2="1620" y1="865" y2="895" color="FontColor" font="VDRSymbols Sans:Book@30" >{PresentShortText}</text>
      <rectangle x1="390" x2="1820" y1="915" y2="920" color="#FF4e78b1" />
 <!-- PRESENT END-->
      
 <!-- NEXT  -->
     <text x1="390" x2="490" y1="940" y2="975" color="FontColor" font="VDRSymbols Sans:Book@35" >{FollowingStartDateTime:%H\:%M}</text>
     <text x1="510" x2="1620" y1="940" y2="975" color="FontColor" font="VDRSymbols Sans:Book@35" condition="not({FollowingHasTimer})" >{FollowingTitle}</text>
     <text x1="510" x2="1620" y1="940" y2="975" color="FontColor" font="VDRSymbols Sans:Book@35" condition="{FollowingHasTimer}" >{FollowingTitle} *</text>
 <!-- NORCHANNELLOGO -->    <text x1="1660" x2="1760" y1="945" y2="975" color="FontColor" align="right" font="VDRSymbols Sans:Book@30" condition="{FollowingDuration}" >{FollowingDuration:%m} </text>
 <!-- NORCHANNELLOGO -->    <text x1="1765" x2="1830" y1="950" y2="975" color="FontColor" align="left" font="VDRSymbols Sans:Book@25" condition="{FollowingDuration}" >мин</text>
     <text x1="510" x2="1620" y1="985" y2="1015" color="FontColor" font="VDRSymbols Sans:Book@30" >{FollowingShortText}</text>
 <!-- NEXT END  -->
     
    </block>
 <!-- LOWER WINDOW 2 END-->

</display>


 <!--###############################################################################################
 #   VOLUME                  ok!                                                                   #
 ################################################################################################-->
 
  <display id="volume" >
 <!-- TOP WINDOW-->
   <window x1="1390" x2="1920" y1="0" y2="70" bpp="4" />
    <rectangle x1="1400" x2="1920" y1="0" y2="70" refresh="all" color="MainLight" />
    <text x1="1410" x2="1840" y1="25" y2="70" refresh="all,update" color="FontColorActive" bgColor="MainLight" align="center" condition="not({IsMute})" font="VDRSymbols Sans:Book@32" >Громкость: {VolumeCurrent}</text>
    <text x1="1410" x2="1840" y1="25" y2="70" refresh="all,update" color="FontColorActive" bgColor="MainLight" align="center" condition="{IsMute}" font="VDRSymbols Sans:Book@32" >Выкл. Звука</text>
    <!-- trans('Mute') -->
 <!-- TOP WINDOW END-->
 
 <!-- BOTTOM WINDOW-->
    <window x1="80" x2="1840" y1="940" y2="1000" bpp="4" />
     <progress   x1="81" x2="1839" y1="941" y2="999" bgColor="Bar" color="BarFill" current="{VolumeCurrent}" total="{VolumeTotal}" />
     <rectangle  x1="80" x2="1840" y1="940" y2="1000" color="BarBlock" condition="{IsMute}" />
 <!-- BOTTOM WINDOW END-->
 
  </display>
  
 <!--###############################################################################################
 #   MESSAGE                         ok!                                                           #
 ################################################################################################-->
 
  <display id="message" >
   <window x1="80" x2="1840" y1="940" y2="1000" bpp="4" />
    <rectangle x1="80" x2="1840" y1="940" y2="1000" refresh="all"  color="MainLight" />
    <rectangle x1="80" x2="1840" y1="940" y2="1000" refresh="all"  condition="{MessageInfo}" color="MainSolid" />
    <rectangle x1="80" x2="1840" y1="940" y2="1000" refresh="all"  condition="{MessageStatus}" color="Status" />
    <rectangle x1="80" x2="1840" y1="940" y2="1000" refresh="all"  condition="{MessageWarning}" color="Warning" />
    <rectangle x1="80" x2="1840" y1="940" y2="1000" refresh="all"  condition="{MessageError}" color="Error" />
    <text      x1="86" x2="1830" y1="940" y2="1000" refresh="all"  color="FontColor" align="center" font="VDRSymbols Sans:Bold@40" >{MessageInfo}</text>
    <text      x1="86" x2="1830" y1="940" y2="1000" refresh="all"  color="FontColor" align="center" font="VDRSymbols Sans:Bold@40" >{MessageStatus}</text>
    <text      x1="86" x2="1830" y1="940" y2="1000" refresh="all"  color="FontColor" align="center" font="VDRSymbols Sans:Bold@40" >{MessageWarning}</text>
    <text      x1="86" x2="1830" y1="940" y2="1000" refresh="all"  color="FontColor" align="center" font="VDRSymbols Sans:Bold@40" >{MessageError}</text>
  </display>
  
 <!--###############################################################################################
 #   REPLAY                             ok!                                                        #
 ################################################################################################-->
  
  <display id="replayInfo" >
 <!-- TOP WINDOW-->
   <window x1="80" x2="1920" y1="0" y2="70" bpp="4" />
   
    <rectangle refresh="all" x1="1400" x2="1920" y1="0" y2="70" color="MainLight" />
    
    <text condition="{IsPlaying}" x1="1410" x2="1840" y1="25" y2="70" align="center" color="FontColorActive" bgColor="MainLight" font="VDRSymbols Sans:Book@35" >Playing</text>
    <!-- trans('Playing') -->
    <text  condition="{IsPausing}" x1="1410" x2="1840" y1="25" y2="70" align="center" color="FontColorActive" bgColor="MainLight" font="VDRSymbols Sans:Book@35" >Pause</text>
    <!-- trans('Pause') -->
    <text    condition="{IsFastForward:0}" x1="1410" x2="1840" y1="25" y2="70" align="center" color="FontColorActive" bgColor="MainLight"  font="VDRSymbols Sans:Book@35" >Fast forward</text>
    <!-- trans('Fast forward') -->
    <text    condition="{IsFastForward:1}" x1="1410" x2="1840" y1="25" y2="70" align="center" color="FontColorActive" bgColor="MainLight" font="VDRSymbols Sans:Book@35" >Fast forward x1 </text>
    <!-- trans('Fast forward') -->
    <text    condition="{IsFastForward:2}" x1="1410" x2="1840" y1="25" y2="70" align="center" color="FontColorActive" bgColor="MainLight" font="VDRSymbols Sans:Book@35" >Fast forward x2</text>
    <!-- trans('Fast forward') -->
    <text    condition="{IsFastForward:3}" x1="1410" x2="1840" y1="25" y2="70" align="center" color="FontColorActive" bgColor="MainLight" font="VDRSymbols Sans:Book@35" >Fast forward x3</text>
    <!-- trans('Fast forward') -->
    <text    condition="{IsFastRewind:0}" x1="1410" x2="1840" y1="25" y2="70" align="center" color="FontColorActive" bgColor="MainLight" font="VDRSymbols Sans:Book@35" >Fast rewind</text>
    <!-- trans('Fast rewind') -->
    <text    condition="{IsFastRewind:1}" x1="1410" x2="1840" y1="25" y2="70" align="center" color="FontColorActive" bgColor="MainLight" font="VDRSymbols Sans:Book@35" >Fast rewind x1 </text>
    <!-- trans('Fast rewind') -->
    <text    condition="{IsFastRewind:2}" x1="1410" x2="1840" y1="25" y2="70" align="center" color="FontColorActive" bgColor="MainLight" font="VDRSymbols Sans:Book@35" >Fast rewind x2</text>
    <!-- trans('Fast rewind') -->
    <text    condition="{IsFastRewind:3}" x1="1410" x2="1840" y1="25" y2="70" align="center" color="FontColorActive" bgColor="MainLight" font="VDRSymbols Sans:Book@35" >Fast rewind x3</text>
    <!-- trans('Fast rewind') -->
    <text    condition="{IsSlowForward:0}" x1="1410" x2="1840" y1="25" y2="70" align="center" color="FontColorActive" bgColor="MainLight" font="VDRSymbols Sans:Book@35" >Slow forward</text>
    <!-- trans('Slow forward') -->
    <text    condition="{IsSlowForward:1}" x1="1410" x2="1840" y1="25" y2="70" align="center" color="FontColorActive" bgColor="MainLight" font="VDRSymbols Sans:Book@35" >Slow forward x1 </text>
    <!-- trans('Slow forward') -->
    <text    condition="{IsSlowForward:2}" x1="1410" x2="1840" y1="25" y2="70" align="center" color="FontColorActive" bgColor="MainLight" font="VDRSymbols Sans:Book@35" >Slow forward x2</text>
    <!-- trans('Slow forward') -->
    <text    condition="{IsSlowForward:3}" x1="1410" x2="1840" y1="25" y2="70" align="center" color="FontColorActive" bgColor="MainLight" font="VDRSymbols Sans:Book@35" >Slow forward x3</text>
    <!-- trans('Slow forward') -->
    <text    condition="{IsSlowRewind:0}" x1="1410" x2="1840" y1="25" y2="70" align="center" color="FontColorActive" bgColor="MainLight" font="VDRSymbols Sans:Book@35" >Slow rewind</text>
    <!-- trans('Slow rewind') -->
    <text    condition="{IsSlowRewind:1}" x1="1410" x2="1840" y1="25" y2="70" align="center" color="FontColorActive" bgColor="MainLight" font="VDRSymbols Sans:Book@35" >Slow rewind x1 </text>
    <!-- trans('Slow rewind') -->
    <text    condition="{IsSlowRewind:2}" x1="1410" x2="1840" y1="25" y2="70" align="center" color="FontColorActive" bgColor="MainLight" font="VDRSymbols Sans:Book@35" >Slow rewind x2</text>
    <!-- trans('Slow rewind') -->
    <text    condition="{IsSlowRewind:3}" x1="1410" x2="1840" y1="25" y2="70" align="center" color="FontColorActive" bgColor="MainLight" font="VDRSymbols Sans:Book@35" >Slow rewind x3</text>
    <!-- trans('Slow rewind') -->
 <!-- TOP WINDOW END-->
        
 <!-- BOTTOM WINDOW-->
   <window x1="80" x2="1840" y1="800" y2="1040" bpp="4" />
   
    <rectangle x1="80" x2="1840" y1="800" y2="870" color="MainLight" />
    <rectangle x1="80" x2="1840" y1="870" y2="950" color="Bar" />
    <text x1="90" x2="1400" y1="817" y2="870" color="FontColor" font="VDRSymbols Sans:Book@35" >{ReplayTitle}</text>
    <text x1="1410" x2="1830" y1="817" y2="870" color="FontColor" align="right" font="VDRSymbols Sans:Book@35" >{ReplayPosition} von {ReplayDuration}</text>
    <!-- trans('of') -->
